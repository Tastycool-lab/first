        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="stylesheet" href="https://bootswatch.com/4/flatly/bootstrap.min.css" >
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
